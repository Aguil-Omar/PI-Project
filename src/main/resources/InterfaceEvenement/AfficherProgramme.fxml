<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #f4f4f4;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.pi_dev.controllers.EvenementController.AfficherProgramme">

    <children>
        <!-- TableView pour afficher les programmes -->
        <TableView fx:id="tableProgrammes" layoutX="50.0" layoutY="50.0" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="colActivite" text="Activité" prefWidth="200.0"/>
                <TableColumn fx:id="colHeureDebut" text="Heure Début" prefWidth="150.0"/>
                <TableColumn fx:id="colHeureFin" text="Heure Fin" prefWidth="150.0"/>
                <TableColumn fx:id="colSupprimer" text="Supprimer" prefWidth="100.0"/>
                <TableColumn fx:id="colModifier" text="Modifier" prefWidth="100.0"/>


            </columns>
        </TableView>
    </children>
</AnchorPane>

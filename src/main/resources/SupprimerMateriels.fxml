<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Materiels.controllers.SupprimerMateriel">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Confirmer la suppression" />


        <Label fx:id="lblMessage" layoutX="20.0" layoutY="60.0" text="Êtes-vous sûr de vouloir supprimer ce matériel ?" />


        <Button fx:id="btnConfirmer" layoutX="50.0" layoutY="120.0" text="Confirmer " onAction="#confirmer" />
        <Button fx:id="btnAnnuler" layoutX="150.0" layoutY="120.0" text="Annuler" onAction="#annuler" />
    </children>
</AnchorPane>